@{
    ViewData["Title"] = "Home Page";
}
<textarea id="message" rows="10" cols="30"></textarea>
<input type="button" onclick="log()" value="写日志" />
@section Scripts{
    <script>
        function log() {

            $.get("/writelog", { sendmessage: $("#message").val() }, function (result) {
              console.log(result)
            })
        }
    </script>

}
